      "Organization": "dnceng",
      "AccessToken": "[vault(dn-bot-dnceng-build-r-code-r-project-r-profile-r)]",
      "MaxParallelRequests": 10
    },
    "dnceng": {
      "Organization": "dnceng",
      "AccessToken": "[vault(dn-bot-dnceng-workitems-rw)]",
      "MaxParallelRequests": 10
    }
  },
